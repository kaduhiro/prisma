{
  "name": "prisma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "serve": "serve ./",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "db:init": "dotenv -e .env.local -- npx prisma init",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "db:reset": "dotenv -e .env.local -- npx prisma db push --force-reset",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "db:rebuild": "yarn db:reset && yarn db:migrate && yarn db:generate"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/font": "13.1.1",
    "@prisma/client": "^4.11.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "http-status-codes": "^2.2.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.6",
    "recoil-persist": "^4.2.0",
    "swr": "^2.1.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.1.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.11.0",
    "serve": "^14.2.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds/post.ts"
  }
}
